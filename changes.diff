+.gallery img{width:100%;border-radius:8px}
+.share{display:flex;gap:.5rem;flex-wrap:wrap}
+.form-inline{display:grid;gap:.5rem;align-items:end}
+.row-actions{display:flex;gap:.5rem;margin-top:.5rem}
+.hidden{display:none}
