+@import url('https://fonts.googleapis.com/css2?family=Poppins:wght@400;600&family=Roboto:wght@400;500&display=swap');
+:root{--azul-primario:#1e88e5;--azul-escuro:#0f4c81;--fundo:#f5f7fa;--texto:#21313a;--borda:#d6e6f6;--radius:12px;--shadow:0 6px 18px rgba(15,76,129,0.12);--maxw:1100px}
