+.features-grid{display:grid;gap:1rem;margin-top:.5rem}
+.feature-card{background:#fff;padding:1rem;border-radius:10px;box-shadow:var(--shadow)}
+.checklist{padding-left:1.2rem}.checklist li{margin:.25rem 0}
+.btn{display:inline-block;padding:.6rem 1rem;border-radius:8px;text-decoration:none;font-weight:700;border:1px solid var(--borda);background:#fff;color:var(--azul-escuro);cursor:pointer}
+.btn-primary{background:linear-gradient(135deg,var(--azul-primario),var(--azul-escuro));color:#fff;border:none;box-shadow:0 6px 16px rgba(15,76,129,.12)}
+/* projetos */
+.projetos-list{margin:1rem 0}.projeto-card{background:#fff;padding:1rem;border-radius:10px;box-shadow:0 6px 16px rgba(15,76,129,.06);margin-bottom:1rem}
+.progress{height:10px;background:#e9f2fb;border-radius:8px;overflow:hidden;margin:.5rem 0}.progress .bar{height:100%;background:linear-gradient(90deg,var(--azul-primario),var(--azul-escuro))}
+.filtro-form{display:grid;gap:.5rem;background:#fff;padding:1rem;border-radius:10px;box-shadow:var(--shadow);margin:1rem 0}
+input,select,textarea{width:100%;padding:.6rem;margin-top:.25rem;border:1px solid var(--borda);border-radius:8px;font-size:1rem}
+input:focus,select:focus,textarea:focus{outline:none;box-shadow:0 0 0 4px rgba(30,136,229,.12);border-color:var(--azul-primario)}
+form.card{background:#fff;padding:1rem;border-radius:10px;box-shadow:var(--shadow)}fieldset{border:1px solid #e6eef8;padding:1rem;border-radius:8px;margin-bottom:1rem}
+legend{font-weight:700;color:var(--azul-escuro);padding:0 .5rem}label{display:block;font-weight:600;margin-top:.5rem}
+.form-group{margin-bottom:.5rem}.form-row{display:grid;grid-template-columns:1fr 1fr;gap:1rem}
+.error{color:#b00020;font-size:.9rem;margin-top:.25rem;min-height:1.1rem}
+footer{background:var(--azul-escuro);color:#fff;text-align:center;padding:1.25rem 0;margin-top:2rem}
+/* Acessibilidade */
+.visually-hidden{position:absolute!important;height:1px;width:1px;overflow:hidden;clip:rect(1px, 1px, 1px, 1px);white-space:nowrap}
