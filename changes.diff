+.tab-panel.hidden{display:none}
+.grid-2{display:grid;gap:1rem}
+@media(min-width:760px){.grid-2{grid-template-columns:1fr 1fr}.hero{grid-template-columns:1fr 420px}.cards-grid{grid-template-columns:repeat(3,1fr)}.features-grid{grid-template-columns:repeat(3,1fr)}}
+@media(max-width:759px){.form-row{grid-template-columns:1fr}}
+.table{width:100%;border-collapse:collapse}.table th,.table td{border-bottom:1px solid #eef3fb;padding:.5rem;text-align:left}
+.gallery{display:grid;grid-template-columns:repeat(auto-fit,minmax(160px,1fr));gap:.75rem}
